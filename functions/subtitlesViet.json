  "body": [
    {
      "from": 0.12,
      "to": 0.82,
      "sid": 1,
      "location": 2,
      "content": "Pháp Nam Minh",
      "music": 0.0
    },
    {
      "from": 0.82,
      "to": 1.64,
      "sid": 2,
      "location": 2,
      "content": "Ly Hỏa Chi Tinh",
      "music": 0.0
    },
    {
      "from": 1.64,
      "to": 2.74,
      "sid": 3,
      "location": 2,
      "content": "Tụ Hỏa Thành Thuật",
      "music": 0.0
    },
    {
      "from": 2.74,
      "to": 5.66,
      "sid": 4,
      "location": 2,
      "content": "Gánh quan tài tiễn đưa hồng trần",
      "music": 0.0
    },
    {
      "from": 5.66,
      "to": 6.88,
      "sid": 5,
      "location": 2,
      "content": "Mở đầu cứu Trường Sinh",
      "music": 0.0
    },
    {
      "from": 6.88,
      "to": 8.08,
      "sid": 6,
      "location": 2,
      "content": "Tiễn đưa để trở nên mạnh mẽ hơn",
      "music": 0.0
    },
    {
      "from": 8.08,
      "to": 9.42,
      "sid": 7,
      "location": 2,
      "content": "Mở đầu với một cuốn \"Tàng Thi Lộ\"",
      "music": 0.0
    },
    {
      "from": 9.42,
      "to": 10.6,
      "sid": 8,
      "location": 2,
      "content": "Một người, một chó trên con đường tu tiên",
      "music": 0.0
    },
    {
      "from": 10.6,
      "to": 13.05,
      "sid": 9,
      "location": 2,
      "content": "Hắn là kẻ đưa tang đi khắp thế gian",
      "music": 0.0
    },
    {
      "from": 13.219,
      "to": 16.739,
      "sid": 10,
      "location": 2,
      "content": "Nhưng giống như hoa khổ, nói nhanh mà tận",
      "music": 0.9999996666667778
    },
    {
      "from": 16.739,
      "to": 19.59,
      "sid": 11,
      "location": 2,
      "content": "Cả đời khó hái",
      "music": 0.31372545943868085
    },
    {
      "from": 19.99,
      "to": 21.25,
      "sid": 12,
      "location": 2,
      "content": "Công công của gia tộc Mặc",
      "music": 0.0
    },
    {
      "from": 21.25,
      "to": 22.83,
      "sid": 13,
      "location": 2,
      "content": "Xem ra Phi Tiên Cốc không còn xa nữa",
      "music": 0.0
    },
    {
      "from": 22.83,
      "to": 25.86,
      "sid": 14,
      "location": 2,
      "content": "Gâu gâu gâu hahaha",
      "music": 0.0
    },
    {
      "from": 25.86,
      "to": 27.38,
      "sid": 15,
      "location": 2,
      "content": "Linh khí bốn phía dày đặc",
      "music": 0.0
    },
    {
      "from": 27.38,
      "to": 29.54,
      "sid": 16,
      "location": 2,
      "content": "Đôi khi trên trời thoáng qua một tia hồng quang",
      "music": 0.0
    },
    {
      "from": 29.54,
      "to": 31.98,
      "sid": 17,
      "location": 2,
      "content": "Rõ ràng là tu sĩ đang phi hành ngang qua",
      "music": 0.0
    },
    {
      "from": 35.4,
      "to": 38.54,
      "sid": 18,
      "location": 2,
      "content": "A a, tốt quá!",
      "music": 0.0
    },
    {
      "from": 38.54,
      "to": 40.02,
      "sid": 19,
      "location": 2,
      "content": "Cuối cùng cũng gặp đồng đạo rồi",
      "music": 0.0
    },
    {
      "from": 40.02,
      "to": 41.16,
      "sid": 20,
      "location": 2,
      "content": "Cả quãng đường không ai nói chuyện",
      "music": 0.0
    },
    {
      "from": 41.16,
      "to": 42.55,
      "sid": 21,
      "location": 2,
      "content": "Chán chết đi được",
      "music": 0.0
    },
    {
      "from": 42.83,
      "to": 44.91,
      "sid": 22,
      "location": 2,
      "content": "Đạo hữu, ngài cũng đang đi đến Tùng Vân Phường sao?",
      "music": 0.0
    },
    {
      "from": 44.91,
      "to": 47.41,
      "sid": 23,
      "location": 2,
      "content": "Vài tháng nữa là đến Đại hội Phi Tiên 5 năm một lần rồi",
      "music": 0.0
    },
    {
      "from": 47.41,
      "to": 48.57,
      "sid": 24,
      "location": 2,
      "content": "Tên này đúng là kẻ lắm lời",
      "music": 0.0
    },
    {
      "from": 48.57,
      "to": 49.73,
      "sid": 25,
      "location": 2,
      "content": "Chẳng đợi Đường nhân đáp lại",
      "music": 0.0
    },
    {
      "from": 49.73,
      "to": 51.47,
      "sid": 26,
      "location": 2,
      "content": "Hắn đã tự mình mở miệng kể liên hồi",
      "music": 0.0
    },
    {
      "from": 51.47,
      "to": 53.09,
      "sid": 27,
      "location": 2,
      "content": "Đạo hữu, tại hạ tên là Vạn Tiểu Phong",
      "music": 0.0
    },
    {
      "from": 53.09,
      "to": 54.73,
      "sid": 28,
      "location": 2,
      "content": "Không biết đạo hữu họ gì tên chi?",
      "music": 0.0
    },
    {
      "from": 54.73,
      "to": 58.51,
      "sid": 29,
      "location": 2,
      "content": "Thái Khôn, chắc hẳn là người Thái gia trên đảo Thái Bình chứ?",
      "music": 0.0
    },
    {
      "from": 59.47,
      "to": 60.67,
      "sid": 30,
      "location": 2,
      "content": "Khôn Khôn huynh đệ à, ngài đang ăn gì vậy?",
      "music": 0.0
    },
    {
      "from": 60.67,
      "to": 62.53,
      "sid": 31,
      "location": 2,
      "content": "Đường nhân thoáng sững sờ",
      "music": 0.0
    },
    {
      "from": 62.53,
      "to": 63.67,
      "sid": 32,
      "location": 2,
      "content": "Không ngờ kẻ này lại tự nhiên đến thế",
      "music": 0.0
    },
    {
      "from": 63.67,
      "to": 64.13,
      "sid": 33,
      "location": 2,
      "content": "Đường nhân xé một cái đùi gà nướng đưa cho Vạn Tiểu Phong",
      "music": 0.0
    },
    {
      "from": 64.13,
      "to": 65.29,
      "sid": 34,
      "location": 2,
      "content": "Vạn Tiểu Phong chẳng khách khí, nhận lấy ăn ngay",
      "music": 0.0
    },
    {
      "from": 65.29,
      "to": 67.37,
      "sid": 35,
      "location": 2,
      "content": "Chẳng chút nghi ngờ rằng đối phương bỏ độc hại mình",
      "music": 0.0
    },
    {
      "from": 67.37,
      "to": 68.63,
      "sid": 36,
      "location": 2,
      "content": "Vạn huynh đệ, ta luôn bế quan khổ tu trong tộc",
      "music": 0.0
    },
    {
      "from": 68.63,
      "to": 69.77,
      "sid": 37,
      "location": 2,
      "content": "Hiểu biết về giới tu tiên cũng không nhiều",
      "music": 0.0
    },
    {
      "from": 69.77,
      "to": 71.88,
      "sid": 38,
      "location": 2,
      "content": "Huynh có thể giảng cho ta nghe một chút được chăng?",
      "music": 0.0
    },
    {
      "from": 74.26,
      "to": 74.98,
      "sid": 39,
      "location": 2,
      "content": "Ta, Vạn Tiểu Phong, được mệnh danh là thông tỏ vạn sự",
      "music": 0.0
    },
    {
      "from": 74.98,
      "to": 76.38,
      "sid": 40,
      "location": 2,
      "content": "Khôn huynh đệ, ngài hỏi đúng người rồi",
      "music": 0.0
    },
    {
      "from": 76.38,
      "to": 78.54,
      "sid": 41,
      "location": 2,
      "content": "Ba môn, đại Lê Quốc",
      "music": 0.0
    },
    {
      "from": 78.54,
      "to": 80.4,
      "sid": 42,
      "location": 2,
      "content": "Giới tu tiên có tổng cộng vài đại thế lực",
      "music": 0.0
    },
    {
      "from": 80.4,
      "to": 82.76,
      "sid": 43,
      "location": 2,
      "content": "Bao gồm sáu phái:",
      "music": 0.0
    },
    {
      "from": 82.76,
      "to": 83.24,
      "sid": 44,
      "location": 2,
      "content": "Thái Ất Kiếm Tông, Tố Nữ Phái, Diệu Đan Các",
      "music": 0.0
    },
    {
      "from": 83.24,
      "to": 84.16,
      "sid": 45,
      "location": 2,
      "content": "Thiên Cung Môn, Lôi Dương Tông, Đại Thiền Tự",
      "music": 0.0
    },
    {
      "from": 84.16,
      "to": 85.0,
      "sid": 46,
      "location": 2,
      "content": "Còn ba đại gia tộc là Lăng gia, Tô gia, và Mộ Dung gia",
      "music": 0.0
    },
    {
      "from": 85.0,
      "to": 86.6,
      "sid": 47,
      "location": 2,
      "content": "Vậy Tùng Vân Phường chúng ta đến lần này thuộc thế lực nào vậy?",
      "music": 0.0
    },
    {
      "from": 86.6,
      "to": 87.52,
      "sid": 48,
      "location": 2,
      "content": "Tùng Vân Phường vốn là một thung lũng mà một đại sư lập ra để thu đồ đệ",
      "music": 0.0
    },
    {
      "from": 87.52,
      "to": 88.44,
      "sid": 49,
      "location": 2,
      "content": "Vì Đại hội Phi Tiên mà được gọi là Phi Tiên Cốc",
      "music": 0.0
    },
    {
      "from": 88.44,
      "to": 89.89,
      "sid": 50,
      "location": 2,
      "content": "Sau đó, nhiều tán tu tụ hội tại đây",
      "music": 0.0
    },
    {
      "from": 89.89,
      "to": 90.73,
      "sid": 51,
      "location": 2,
      "content": "Hình thành một phường thị nhỏ",
      "music": 0.0
    },
    {
      "from": 90.73,
      "to": 91.59,
      "sid": 52,
      "location": 2,
      "content": "Nơi này do người Bích Lạc Thành quản lý",
      "music": 0.0
    },
    {
      "from": 91.59,
      "to": 92.43,
      "sid": 53,
      "location": 2,
      "content": "Phần lớn thời gian Vạn Tiểu Phong nói",
      "music": 0.0
    },
    {
      "from": 92.43,
      "to": 96.08,
      "sid": 54,
      "location": 2,
      "content": "Đường nhân chỉ lặng lẽ lắng nghe",
      "music": 0.0
    },
    {
      "from": 96.08,
      "to": 97.46,
      "sid": 55,
      "location": 2,
      "content": "Một lúc lâu sau...",
      "music": 0.0
    },
    {
      "from": 97.46,
      "to": 99.12,
      "sid": 56,
      "location": 2,
      "content": "Vạn huynh đệ, không biết huynh bình thường thích gì?",
      "music": 0.0
    },
    {
      "from": 99.12,
      "to": 101.15,
      "sid": 57,
      "location": 2,
      "content": "Thích nhiều thứ lắm: mỹ thực, du sơn ngoạn thủy",
      "music": 0.0
    },
    {
      "from": 101.15,
      "to": 102.99,
      "sid": 58,
      "location": 2,
      "content": "Đặc biệt thích nghe kỳ văn dị sự",
      "music": 0.0
    },
    {
      "from": 102.99,
      "to": 104.91,
      "sid": 59,
      "location": 2,
      "content": "Nhưng đáng tiếc, trưởng bối trong nhà không cho phép làm mấy chuyện đó",
      "music": 0.0
    },
    {
      "from": 106.62,
      "to": 108.7,
      "sid": 60,
      "location": 2,
      "content": "Còn Khôn Khôn huynh đệ, ngài có sở thích gì không?",
      "music": 0.0
    },
    {
      "from": 108.7,
      "to": 110.82,
      "sid": 61,
      "location": 2,
      "content": "Thích đưa tang đấy!",
      "music": 0.0
    },
    {
      "from": 110.82,
      "to": 113.9,
      "sid": 62,
      "location": 2,
      "content": "Vạn huynh đệ, ngày nào đó nếu ngài ngã xuống",
      "music": 0.0
    },
    {
      "from": 113.9,
      "to": 115.54,
      "sid": 63,
      "location": 2,
      "content": "Ta có thể tìm một nơi phong thủy bảo địa",
      "music": 0.0
    },
    {
      "from": 115.54,
      "to": 116.86,
      "sid": 64,
      "location": 2,
      "content": "Làm trọn gói mọi việc từ đưa tang đến mai táng cho ngài",
      "music": 0.0
    },
    {
      "from": 116.86,
      "to": 118.87,
      "sid": 65,
      "location": 2,
      "content": "Khôn Khôn huynh đệ, sở thích của ngài đúng là thú vị thật",
      "music": 0.0
    },
    {
      "from": 118.87,
      "to": 120.63,
      "sid": 66,
      "location": 2,
      "content": "Đợi sau khi ta chết",
      "music": 0.0
    },
    {
      "from": 120.63,
      "to": 122.979,
      "sid": 67,
      "location": 2,
      "content": "Nhất định nhờ ngài tìm một nơi tốt để an táng cho ta",
      "music": 0.0
    },
    {
      "from": 122.979,
      "to": 125.199,
      "sid": 68,
      "location": 2,
      "content": "Ta thấy nơi này cũng không tệ lắm đâu",
      "music": 0.0
    },
    {
      "from": 125.199,
      "to": 127.819,
      "sid": 69,
      "location": 2,
      "content": "Có lẽ là một nơi mai táng lý tưởng",
      "music": 0.0
    },
    {
      "from": 129.599,
      "to": 130.319,
      "sid": 70,
      "location": 2,
      "content": "Khôn Khôn huynh đệ, ngươi... ngươi!",
      "music": 0.0
    },
    {
      "from": 130.319,
      "to": 132.24,
      "sid": 71,
      "location": 2,
      "content": "Ra đây đi! Hay là phải để ta mời ngươi ra?",
      "music": 0.0
    },
    {
      "from": 132.24,
      "to": 133.54,
      "sid": 72,
      "location": 2,
      "content": "Tiểu tử, bộ Hoàng Thổ Y của ta",
      "music": 0.0
    },
    {
      "from": 133.54,
      "to": 135.1,
      "sid": 73,
      "location": 2,
      "content": "Có thể thu liễm khí tức, hòa mình vào đất đá",
      "music": 0.0
    },
    {
      "from": 135.1,
      "to": 136.78,
      "sid": 74,
      "location": 2,
      "content": "Ngươi làm thế nào nhìn ra được ngụy trang của ta?",
      "music": 0.0
    },
    {
      "from": 136.78,
      "to": 138.76,
      "sid": 75,
      "location": 2,
      "content": "Vạn Tiểu Phong vẫn luôn ẩn giấu rất giỏi",
      "music": 0.0
    },
    {
      "from": 138.76,
      "to": 139.72,
      "sid": 76,
      "location": 2,
      "content": "Không để lộ chút sơ hở nào",
      "music": 0.0
    },
    {
      "from": 139.72,
      "to": 141.31,
      "sid": 77,
      "location": 2,
      "content": "Nhưng đến lúc vào hẻm núi",
      "music": 0.0
    },
    {
      "from": 141.31,
      "to": 143.55,
      "sid": 78,
      "location": 2,
      "content": "Hắn lại vô tình để lộ sát ý",
      "music": 0.0
    },
    {
      "from": 143.55,
      "to": 145.63,
      "sid": 79,
      "location": 2,
      "content": "Đáng tiếc, Đường nhân từng trải qua Thần Biến Cảnh",
      "music": 0.0
    },
    {
      "from": 145.63,
      "to": 147.77,
      "sid": 80,
      "location": 2,
      "content": "Lục giác thông linh, đặc biệt mẫn cảm với sát ý",
      "music": 0.0
    },
    {
      "from": 147.77,
      "to": 149.25,
      "sid": 81,
      "location": 2,
      "content": "Thêm nữa, hắn là phong thủy sư tam giai",
      "music": 0.0
    },
    {
      "from": 150.29,
      "to": 152.39,
      "sid": 82,
      "location": 2,
      "content": "Thần hồn đã được địa khí rèn luyện",
      "music": 0.0
    },
    {
      "from": 152.39,
      "to": 153.17,
      "sid": 83,
      "location": 2,
      "content": "Mạnh hơn người thường",
      "music": 0.0
    },
    {
      "from": 153.17,
      "to": 155.71,
      "sid": 84,
      "location": 2,
      "content": "Tiểu tử, ngươi dám giết đệ đệ ta",
      "music": 0.0
    },
    {
      "from": 155.71,
      "to": 157.37,
      "sid": 85,
      "location": 2,
      "content": "Ta phải luyện hồn ngươi, thu hồn luyện phách",
      "music": 0.0
    },
    {
      "from": 157.37,
      "to": 161.44,
      "sid": 86,
      "location": 2,
      "content": "Ta khuyên ngươi tự mình đầu hàng",
      "music": 0.0
    },
    {
      "from": 161.44,
      "to": 162.66,
      "sid": 87,
      "location": 2,
      "content": "Bớt chịu khổ",
      "music": 0.0
    },
    {
      "from": 162.66,
      "to": 166.98,
      "sid": 88,
      "location": 2,
      "content": "Ta cũng có một đề nghị",
      "music": 0.2608695368620042
    },
    {
      "from": 169.68,
      "to": 170.54,
      "sid": 89,
      "location": 2,
      "content": "Chi bằng ngươi tự sát ngay bây giờ",
      "music": 0.0
    },
    {
      "from": 170.54,
      "to": 172.8,
      "sid": 90,
      "location": 2,
      "content": "Ta bảo đảm sẽ lo chu toàn việc mai táng",
      "music": 0.0
    },
    {
      "from": 173.33,
      "to": 176.269,
      "sid": 91,
      "location": 2,
      "content": "Kẻ thu xác ngông cuồng!",
      "music": 0.0
    },
    {
      "from": 176.269,
      "to": 177.649,
      "sid": 92,
      "location": 2,
      "content": "Ra tay đi, xem ngươi có bản lĩnh gì",
      "music": 0.0
    },
    {
      "from": 177.649,
      "to": 179.769,
      "sid": 93,
      "location": 2,
      "content": "Đường nhân chỉ cười nhạt",
      "music": 0.0
    },
    {
      "from": 182.26,
      "to": 183.92,
      "sid": 94,
      "location": 2,
      "content": "Nhẹ nhàng vỗ tay",
      "music": 0.0
    },
    {
      "from": 183.92,
      "to": 185.26,
      "sid": 95,
      "location": 2,
      "content": "Đất đá xung quanh rung động",
      "music": 0.0
    },
    {
