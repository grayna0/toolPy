{
  "font_size": 0.4,
  "font_color": "#FFFFFF",
  "background_alpha": 0.5,
  "background_color": "#9C27B0",
  "Stroke": "none",
  "type": "AIsubtitle",
  "lang": "zh",
  "version": "v1.7.0.4",
  "body": [
    {
      "from": 0.12,
      "to": 0.82,
      "sid": 1,
      "location": 2,
      "content": "Pháp Nam Minh.",
      "music": 0.0
    },
    {
      "from": 0.82,
      "to": 1.64,
      "sid": 2,
      "location": 2,
      "content": "Tinh hoa ly hỏa.",
      "music": 0.0
    },
    {
      "from": 1.64,
      "to": 2.74,
      "sid": 3,
      "location": 2,
      "content": "Tụ hỏa thành thuật.",
      "music": 0.0
    },
    {
      "from": 2.74,
      "to": 5.66,
      "sid": 4,
      "location": 2,
      "content": "Người đưa tang chốn hồng trần gánh người.",
      "music": 0.0
    },
    {
      "from": 5.66,
      "to": 6.88,
      "sid": 5,
      "location": 2,
      "content": "Mở đầu là cứu Trường Sinh.",
      "music": 0.0
    },
    {
      "from": 6.88,
      "to": 8.08,
      "sid": 6,
      "location": 2,
      "content": "Đưa tang là mạnh lên.",
      "music": 0.0
    },
    {
      "from": 8.08,
      "to": 9.42,
      "sid": 7,
      "location": 2,
      "content": "Khởi đầu với một cuốn sách Tàng Thi Lộ.",
      "music": 0.0
    },
    {
      "from": 9.42,
      "to": 10.6,
      "sid": 8,
      "location": 2,
      "content": "Một người một chó, con đường tu tiên.",
      "music": 0.0
    },
    {
      "from": 10.6,
      "to": 13.05,
      "sid": 9,
      "location": 2,
      "content": "Hắn là kẻ đưa tang đi khắp nhân gian.",
      "music": 0.0
    },
    {
      "from": 13.219,
      "to": 16.739,
      "sid": 10,
      "location": 2,
      "content": "Chỉ là như lời Hoa Khổ nói, làm nhanh lên.",
      "music": 0.9999996666667778
    },
    {
      "from": 16.739,
      "to": 19.59,
      "sid": 11,
      "location": 2,
      "content": "Cả đời khó hái.",
      "music": 0.31372545943868085
    },
    {
      "from": 19.99,
      "to": 21.25,
      "sid": 12,
      "location": 2,
      "content": "Công công công, lão Mạc.",
      "music": 0.0
    },
    {
      "from": 21.25,
      "to": 22.83,
      "sid": 13,
      "location": 2,
      "content": "Xem ra không còn xa Phi Tiên Cốc.",
      "music": 0.0
    },
    {
      "from": 22.83,
      "to": 25.86,
      "sid": 14,
      "location": 2,
      "content": "Gâu gâu gâu, haha!",
      "music": 0.0
    },
    {
      "from": 25.86,
      "to": 27.38,
      "sid": 15,
      "location": 2,
      "content": "Linh khí xung quanh dày đặc.",
      "music": 0.0
    },
    {
      "from": 27.38,
      "to": 29.54,
      "sid": 16,
      "location": 2,
      "content": "Thỉnh thoảng trên trời lại xẹt qua từng tia hồng quang.",
      "music": 0.0
    },
    {
      "from": 29.54,
      "to": 31.98,
      "sid": 17,
      "location": 2,
      "content": "Rõ ràng là các tu sĩ đang gấp rút qua đường.",
      "music": 0.0
    },
    {
      "from": 35.4,
      "to": 38.54,
      "sid": 18,
      "location": 2,
      "content": "A a, tốt quá rồi!",
      "music": 0.0
    },
    {
      "from": 38.54,
      "to": 40.02,
      "sid": 19,
      "location": 2,
      "content": "Cuối cùng cũng gặp được đồng đạo.",
      "music": 0.0
    },
    {
      "from": 40.02,
      "to": 41.16,
      "sid": 20,
      "location": 2,
      "content": "Trên đường này chẳng ai nói chuyện.",
      "music": 0.0
    },
    {
      "from": 41.16,
      "to": 42.55,
      "sid": 21,
      "location": 2,
      "content": "Chán chết tôi mất.",
      "music": 0.0
    },
    {
      "from": 42.83,
      "to": 44.91,
      "sid": 22,
      "location": 2,
      "content": "Đạo hữu, ngươi cũng đến Tùng Vân Phường phải không?",
      "music": 0.0
    },
    {
      "from": 44.91,
      "to": 47.41,
      "sid": 23,
      "location": 2,
      "content": "Vài tháng nữa là Đại hội Phi Tiên năm năm một lần.",
      "music": 0.0
    },
    {
      "from": 47.41,
      "to": 48.57,
      "sid": 24,
      "location": 2,
      "content": "Tên này đúng là một kẻ lắm mồm.",
      "music": 0.0
    },
    {
      "from": 48.57,
      "to": 49.73,
      "sid": 25,
      "location": 2,
      "content": "Chưa đợi Đường Nhân đáp lời.",
      "music": 0.0
    },
    {
      "from": 49.73,
      "to": 51.47,
      "sid": 26,
      "location": 2,
      "content": "Hắn đã tự mở hộp chuyện.",
      "music": 0.0
    },
    {
      "from": 51.47,
      "to": 53.09,
      "sid": 27,
      "location": 2,
      "content": "Đạo hữu, ta tên là Vạn Tiểu Phong.",
      "music": 0.0
    },
    {
      "from": 53.09,
      "to": 54.73,
      "sid": 28,
      "location": 2,
      "content": "Không biết đạo hữu họ gì, tên chi?",
      "music": 0.0
    },
    {
      "from": 54.73,
      "to": 58.51,
      "sid": 29,
      "location": 2,
      "content": "Thái Khôn, chắc hẳn là nhà Thái đảo Thái Bình nhỉ?",
      "music": 0.0
    },
    {
      "from": 59.47,
      "to": 60.67,
      "sid": 30,
      "location": 2,
      "content": "Huynh đệ Khôn Khôn à!",
      "music": 0.0
    },
    {
      "from": 60.67,
      "to": 62.53,
      "sid": 31,
      "location": 2,
      "content": "Ngươi đang ăn gì thế?",
      "music": 0.0
    },
    {
      "from": 62.53,
      "to": 63.67,
      "sid": 32,
      "location": 2,
      "content": "Đường Nhân ngạc nhiên.",
      "music": 0.0
    },
    {
      "from": 63.67,
      "to": 64.13,
      "sid": 33,
      "location": 2,
      "content": "Không ngờ được.",
      "music": 0.0
    },
    {
      "from": 64.13,
      "to": 65.29,
      "sid": 34,
      "location": 2,
      "content": "Tên này đúng là kiểu tự nhiên như ruồi.",
      "music": 0.0
    },
    {
      "from": 65.29,
      "to": 67.37,
      "sid": 35,
      "location": 2,
      "content": "Đường Nhân chia một đùi gà nướng cho Vạn Tiểu Phong.",
      "music": 0.0
    },
    {
      "from": 67.37,
      "to": 68.63,
      "sid": 36,
      "location": 2,
      "content": "Vạn Tiểu Phong cũng không khách sáo.",
      "music": 0.0
    },
    {
      "from": 68.63,
      "to": 69.77,
      "sid": 37,
      "location": 2,
      "content": "Cầm lấy liền ăn ngay.",
      "music": 0.0
    },
    {
      "from": 69.77,
      "to": 71.88,
      "sid": 38,
      "location": 2,
      "content": "Hoàn toàn không sợ Đường Nhân hạ độc hại hắn.",
      "music": 0.0
    },
    {
      "from": 74.26,
      "to": 74.98,
      "sid": 39,
      "location": 2,
      "content": "Vạn huynh đệ!",
      "music": 0.0
    },
    {
      "from": 74.98,
      "to": 76.38,
      "sid": 40,
      "location": 2,
      "content": "Ta luôn khổ luyện trong tộc.",
      "music": 0.0
    },
    {
      "from": 76.38,
      "to": 78.54,
      "sid": 41,
      "location": 2,
      "content": "Không hiểu biết nhiều về giới tu tiên.",
      "music": 0.0
    },
    {
      "from": 78.54,
      "to": 80.4,
      "sid": 42,
      "location": 2,
      "content": "Ngươi có thể kể cho ta nghe chút được không?",
      "music": 0.0
    },
    {
      "from": 80.4,
      "to": 82.76,
      "sid": 43,
      "location": 2,
      "content": "Ta, Vạn Tiểu Phong, được gọi là Vạn sự thông đây!",
      "music": 0.0
    },
    {
      "from": 82.76,
      "to": 83.24,
      "sid": 44,
      "location": 2,
      "content": "Khôn huynh đệ.",
      "music": 0.0
    },
    {
      "from": 83.24,
      "to": 84.16,
      "sid": 45,
      "location": 2,
      "content": "Ngươi hỏi đúng người rồi.",
      "music": 0.0
    },
    {
      "from": 84.16,
      "to": 85.0,
      "sid": 46,
      "location": 2,
      "content": "Tam môn Đại Ly Quốc.",
      "music": 0.0
    },
    {
      "from": 85.0,
      "to": 86.6,
      "sid": 47,
      "location": 2,
      "content": "Giới tu tiên có mấy thế lực lớn.",
      "music": 0.0
    },
    {
      "from": 86.6,
      "to": 87.52,
      "sid": 48,
      "location": 2,
      "content": "Lần lượt là sáu phái.",
      "music": 0.0
    },
    {
      "from": 87.52,
      "to": 88.44,
      "sid": 49,
      "location": 2,
      "content": "1×3 đại gia.",
      "music": 0.0
    },
    {
      "from": 88.44,
      "to": 89.89,
      "sid": 50,
      "location": 2,
      "content": "Sáu phái là Thái Ất Kiếm Tông,",
      "music": 0.0
    },
    {
      "from": 89.89,
      "to": 90.73,
      "sid": 51,
      "location": 2,
      "content": "Tố Nữ Phái,",
      "music": 0.0
    },
    {
      "from": 90.73,
      "to": 91.59,
      "sid": 52,
      "location": 2,
      "content": "Diệu Đan Các,",
      "music": 0.0
    },
    {
      "from": 91.59,
      "to": 92.43,
      "sid": 53,
      "location": 2,
      "content": "Thiên Cung Môn,",
      "music": 0.0
    },
    {
      "from": 92.43,
      "to": 96.08,
      "sid": 54,
      "location": 2,
      "content": "và Lôi Dương Tổng Hợp, bao gồm Đại Thiền Tự, Dịch Thành, Thấp Bích Lạc Thành, và Bích Lạc Thành.",
      "music": 0.0
    },
    {
      "from": 96.08,
      "to": 97.46,
      "sid": 55,
      "location": 2,
      "content": "Nằm sâu trong vùng Thập Vạn Đại Sơn.",
      "music": 0.0
    },
    {
      "from": 97.46,
      "to": 99.12,
      "sid": 56,
      "location": 2,
      "content": "Đây là siêu đô thị duy nhất được xây dựng bởi một Nguyên Anh tán tu,",
      "music": 0.0
    },
    {
      "from": 99.12,
      "to": 101.15,
      "sid": 57,
      "location": 2,
      "content": "lão tổ Bích Lạc sáng lập.",
      "music": 0.0
    },
    {
      "from": 101.15,
      "to": 102.99,
      "sid": 58,
      "location": 2,
      "content": "Tam đại gia bao gồm Lăng Gia Công,",
      "music": 0.0
    },
    {
      "from": 102.99,
      "to": 104.91,
      "sid": 59,
      "location": 2,
      "content": "Thư Gia và Mộ Dung Gia.",
      "music": 0.0
    },
    {
      "from": 106.62,
      "to": 108.7,
      "sid": 60,
      "location": 2,
      "content": "Vậy lần này chúng ta đến Tùng Vân Phường,",
      "music": 0.0
    },
    {
      "from": 108.7,
      "to": 110.82,
      "sid": 61,
      "location": 2,
      "content": "không biết thuộc thế lực nào nhỉ?",
      "music": 0.0
    },
    {
      "from": 110.82,
      "to": 113.9,
      "sid": 62,
      "location": 2,
      "content": "Tùng Vân Phường vốn là một thung lũng nơi đại sư Lợi nhận đồ đệ.",
      "music": 0.0
    },
    {
      "from": 113.9,
      "to": 115.54,
      "sid": 63,
      "location": 2,
      "content": "Vì lý do Đại hội Phi Tiên,",
      "music": 0.0
    },
    {
      "from": 115.54,
      "to": 116.86,
      "sid": 64,
      "location": 2,
      "content": "nên được gọi là Phi Tiên Cốc.",
      "music": 0.0
    },
    {
      "from": 116.86,
      "to": 118.87,
      "sid": 65,
      "location": 2,
      "content": "Sau đó tự nhiên tập hợp nhiều tán tu,",
      "music": 0.0
    },
    {
      "from": 118.87,
      "to": 120.63,
      "sid": 66,
      "location": 2,
      "content": "hình thành một phường nhỏ.",
      "music": 0.0
    },
    {
      "from": 120.63,
      "to": 122.979,
      "sid": 67,
      "location": 2,
      "content": "Phường này do người từ Bích Lạc Thành quản lý.",
      "music": 0.0
    },
    {
      "from": 122.979,
      "to": 125.199,
      "sid": 68,
      "location": 2,
      "content": "Hầu hết thời gian là Vạn Tiểu Phong nói,",
      "music": 0.0
    },
    {
      "from": 125.199,
      "to": 127.819,
      "sid": 69,
      "location": 2,
      "content": "Đường Nhân ngồi nghe nửa ngày,",
      "music": 0.0
    },
    {
      "from": 129.599,
      "to": 130.319,
      "sid": 70,
      "location": 2,
      "content": "Vạn huynh đệ.",
      "music": 0.0
    },
    {
      "from": 130.319,
      "to": 132.24,
      "sid": 71,
      "location": 2,
      "content": "Không biết thường ngày ngươi thích gì?",
      "music": 0.0
    },
    {
      "from": 132.24,
      "to": 133.54,
      "sid": 72,
      "location": 2,
      "content": "Thích thì nhiều lắm.",
      "music": 0.0
    },
    {
      "from": 133.54,
      "to": 135.1,
      "sid": 73,
      "location": 2,
      "content": "Mỹ thực, du ngoạn sơn thủy.",
      "music": 0.0
    },
    {
      "from": 135.1,
      "to": 136.78,
      "sid": 74,
      "location": 2,
      "content": "Đặc biệt thích nghe kỳ văn dị sự.",
      "music": 0.0
    },
    {
      "from": 136.78,
      "to": 138.76,
      "sid": 75,
      "location": 2,
      "content": "Đáng tiếc trưởng bối trong nhà không cho làm mấy thứ này.",
      "music": 0.0
    },
    {
      "from": 138.76,
      "to": 139.72,
      "sid": 76,
      "location": 2,
      "content": "Khôn Khôn huynh đệ.",
      "music": 0.0
    },
    {
      "from": 139.72,
      "to": 141.31,
      "sid": 77,
      "location": 2,
      "content": "Còn ngươi, có sở thích gì?",
      "music": 0.0
    },
    {
      "from": 141.31,
      "to": 143.55,
      "sid": 78,
      "location": 2,
      "content": "Thích đưa tang cho người ta đó.",
      "music": 0.0
    },
    {
      "from": 143.55,
      "to": 145.63,
      "sid": 79,
      "location": 2,
      "content": "Vạn huynh đệ, ngày nào đó ngươi mất,",
      "music": 0.0
    },
    {
      "from": 145.63,
      "to": 147.77,
      "sid": 80,
      "location": 2,
      "content": "ta có thể tìm cho huynh một mảnh đất phong thủy bảo địa.",
      "music": 0.0
    },
    {
      "from": 147.77,
      "to": 149.25,
      "sid": 81,
      "location": 2,
      "content": "Mọi dịch vụ trọn gói đều có.",
      "music": 0.0
    },
    {
      "from": 150.29,
      "to": 152.39,
      "sid": 82,
      "location": 2,
      "content": "Khôn Khôn huynh đệ, sở thích này thú vị thật.",
      "music": 0.0
    },
    {
      "from": 152.39,
      "to": 153.17,
      "sid": 83,
      "location": 2,
      "content": "Đợi ta chết rồi,",
      "music": 0.0
    },
    {
      "from": 153.17,
      "to": 155.71,
      "sid": 84,
      "location": 2,
      "content": "ta nhất định nhờ ngươi tìm chỗ tốt để chôn cất.",
      "music": 0.0
    },
    {
      "from": 155.71,
      "to": 157.37,
      "sid": 85,
      "location": 2,
      "content": "Ta thấy chỗ này cũng không tồi đâu.",
      "music": 0.0
    },
    {
      "from": 157.37,
      "to": 161.44,
      "sid": 86,
      "location": 2,
      "content": "Ta nghĩ nơi đây chính là một mộ phần lý tưởng.",
      "music": 0.0
    },
    {
      "from": 161.44,
      "to": 162.66,
      "sid": 87,
      "location": 2,
      "content": "Khôn Khôn huynh đệ.",
      "music": 0.0
    },
    {
      "from": 162.66,
      "to": 166.98,
      "sid": 88,
      "location": 2,
      "content": "Ngươi, ngươi...",
      "music": 0.2608695368620042
    },
    {
      "from": 169.68,
      "to": 170.54,
      "sid": 89,
      "location": 2,
      "content": "Ra đây đi.",
      "music": 0.0
    },
    {
      "from": 170.54,
      "to": 172.8,
      "sid": 90,
      "location": 2,
      "content": "Chẳng lẽ để ta mời ngươi ra sao?",
      "music": 0.0
    },
    {
      "from": 173.33,
      "to": 176.269,
      "sid": 91,
      "location": 2,
      "content": "Tiểu tử, Hoàng Thổ Y của ta có thể thu liễm khí tức,",
      "music": 0.0
    },
    {
      "from": 176.269,
      "to": 177.649,
      "sid": 92,
      "location": 2,
      "content": "hòa làm một với đất đá.",
      "music": 0.0
    },
    {
      "from": 177.649,
      "to": 179.769,
      "sid": 93,
      "location": 2,
      "content": "Ngươi làm sao nhìn thấu ngụy trang của ta?",
      "music": 0.0
    },
    {
      "from": 182.26,
      "to": 183.92,
      "sid": 94,
      "location": 2,
      "content": "Vạn Tiểu Phong luôn che giấu rất kỹ,",
      "music": 0.0
    },
    {
      "from": 183.92,
      "to": 185.26,
      "sid": 95,
      "location": 2,
      "content": "không để lộ chút sơ hở nào.",
      "music": 0.0
    },
    {
      "from": 185.26,
      "to": 186.58,
      "sid": 96,
      "location": 2,
      "content": "Nhưng đến hẻm núi này,",
      "music": 0.0
    },
    {
      "from": 186.58,
      "to": 187.84,
      "sid": 97,
      "location": 2,
      "content": "hắn để lộ một chút sát ý.",
      "music": 0.0
    },
    {
      "from": 187.84,
      "to": 189.78,
      "sid": 98,
      "location": 2,
      "content": "Đáng tiếc Đường Nhân từng trải qua Thần Biến Cảnh,",
      "music": 0.0
    },
    {
      "from": 189.78,
      "to": 190.64,
      "sid": 99,
      "location": 2,
